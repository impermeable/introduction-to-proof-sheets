(* Definition 5.1.30: Antisymmetric relation *)
Definition antisymmetric {X : Type} (R : X → X → Prop) : Prop :=
  ∀ a ∈ X, ∀ b ∈ X, R a b → R b a → a = b.

(* Notation for antisymmetric relations *)
Notation "R 'is' 'antisymmetric'" := (antisymmetric R) (at level 68) : function_scope.
